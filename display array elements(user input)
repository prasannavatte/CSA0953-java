import java.util.Scanner;

class Testarray {
    public static void main(String[] args) {
        int a[] = new int[4];
        Scanner s = new Scanner(System.in);
        System.out.println("Enter the elements");
        for (int i = 0; i < a.length; i++) {
            a[i] = s.nextInt();
        }
    }
}
